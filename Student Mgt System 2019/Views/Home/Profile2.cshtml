@model System.Data.DataTable
@{
    ViewBag.Title = "Profile";
}



<div class="row">
    <div class="col-md-12">

     


@for (int i = 0; i < Model.Rows.Count; i++)
    {
        <h2 style="text-align:center;">@Model.Rows[i]["profile_title"]</h2>
    
    <h3 >Collaborators </h3>
    <p >@Model.Rows[i]["profile_collaborator"]</p>
    <h3 >Industrial Collaborators</h3>
    <p >@Model.Rows[i]["profile_industrial_collaborator"]</p>
    
    <h3 ></h3>
    <p >@Model.Rows[i]["profile_description"]</p>
    
    
     <img src="/Assets/@Model.Rows[i]["profile_image"]" width="60%"  />
    
    
        
    
    
}
        

 

    </div>
</div>